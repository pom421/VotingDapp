{
  "name": "votingdapp",
  "version": "1.0.0",
  "description": "A dapp for DAO voting",
  "main": "index.js",
  "scripts": {
    "install": "cd client && npm install && cd ../truffle && npm install",
    "client:build": "cd client && npm run build",
    "client:start": "cd client && npm run start",
    "client:test": "echo \"Error: no test specified\" && exit 1",
    "truffle:compile": "cd truffle && npm run compile",
    "truffle:migrate": "cd truffle && npm run migrate",
    "truffle:migrate:goerli": "cd truffle && npm run migrate -- --network goerli",
    "truffle:test": "cd truffle && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pom421/VotingDapp.git"
